import com.greensock.TweenMax;import com.greensock.easing.*;var R:Number = 140;var Max:int = 20;var rRange:Number = 7;var duration:Number = 4;var boxArr:Array = [];var shell:MovieClip = new MovieClip();addChild(shell);TweenMax.to(shell, 0, {x:200, y:200});function creatBox(_color:uint = 0, _r:Number = 10):Shape{	var foo:Shape = new Shape();	foo.graphics.beginFill(_color, 1);	foo.graphics.drawRect(-_r, -_r, _r * 2, _r * 2);	return foo;}for(var i:int = 0; i < Max; i++){	boxArr.push(creatBox(0xFFFFFF * (1 - i % 2), R - rRange * i));	shell.addChild(boxArr[i]);}function run(){	for(var i:int = 0; i < Max; i++)	{		TweenMax.to(boxArr[i], duration, {rotation:(i + 1) * 90, ease:Cubic.easeInOut});	}}run();