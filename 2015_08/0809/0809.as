import com.greensock.TweenMax;import com.greensock.easing.*;var i:int;var rgbArr:Array = [];var box:MovieClip = new MovieClip();var RGB:BitmapData = creatBitmap();var count:Number = 0;var tar:Shape = new Shape();addChild(box);box.x = 200, box.y = 200;var white = new Shape();white.graphics.beginFill(0xFFFFFF, 1);white.graphics.drawRect(-200, -200, 400, 400);TweenMax.to(white, 0, {alpha:0, x:200, y:200});addChild(white);function creatRGB():Shape{	var foo:Shape = new Shape();	foo.graphics.beginFill(0xFF0000, 1);	foo.graphics.drawRect(0, 0, 1, 3);	foo.graphics.beginFill(0x00FF00, 1);	foo.graphics.drawRect(1, 0, 1, 3);	foo.graphics.beginFill(0x0000FF, 1);	foo.graphics.drawRect(2, 0, 1, 3);	foo.graphics.endFill();	return foo;}function init(){	for(i = 0; i < 10000; i++)	{		var foo:MovieClip = new MovieClip();		foo.addChild(new Bitmap(RGB));		rgbArr.push(foo);		box.addChild(rgbArr[i]);		rgbArr[i].x = i % 100 * 4 - 200;		rgbArr[i].y = int(i / 100) * 4 - 200;		rgbArr[i].alpha = Math.random();		rgbArr[i].id = Math.sin(Math.sqrt(Math.pow(i % 100 * 4 - 200, 2) + Math.pow(int(i / 100) * 4 - 200, 2) * Math.PI / 180));	}	addEventListener(Event.ENTER_FRAME, follow);		TweenMax.delayedCall(4, run);}init();function creatBitmap():BitmapData{	var foo:MovieClip = new MovieClip();	foo.addChild(creatRGB());		var bmd:BitmapData = new BitmapData(3, 3, false, 0xFFFFFF);	bmd.draw(foo);	return bmd;		var bm:Bitmap = new Bitmap(bmd);	bm.smoothing = true;}function follow(e:Event){	count -= 10;	for(i = 0; i < 10000; i++)	{		//trace(rgbArr[i].id + count % 360)		rgbArr[i].alpha = Math.random();			}}function run(){	removeEventListener(Event.ENTER_FRAME, follow);	TweenMax.to(box, 0.1, {scaleY:0});	TweenMax.to(white, 0.1, {alpha:1, height:4});	TweenMax.to(white, 0.2, {height:2, width:2, delay:0.15, overwrite:false});	TweenMax.to(white, 0, {visible:false, delay:0.4, overwrite:false});}