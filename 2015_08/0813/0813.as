addEventListener(Event.ENTER_FRAME, drawLine);function drawLine(e:Event){	this.graphics.clear();	this.graphics.lineStyle(1, 0xFFFFFF, 0);	this.graphics.beginFill(0xFFFFFF, 1);	this.graphics.moveTo(Math.random() * 200 + 100, Math.random() * 200 + 100);	for(var i:int = 0; i < 120; i++)	{		this.graphics.curveTo(Math.random() * 200 + 100, Math.random() * 200 + 100, Math.random() * 200 + 100, Math.random() * 200 + 100);	}		this.graphics.endFill();	this.graphics.beginFill(0, 1);	this.graphics.drawRect(0, 0, 400, 400);	this.graphics.drawCircle(200, 200, 60);}