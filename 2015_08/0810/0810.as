import com.greensock.TweenMax;import com.greensock.easing.*;stage.color = 0x888888;var footArr:Array = [];var shadowArr:Array = [];var xArr:Array = [];var yArr:Array = [];var order:Array = [0, 3, 1, 2];function creatFoot(_fc:Number = 0):Shape{	var foo:Shape = new Shape();	foo.graphics.lineStyle(2, 0, 0);	foo.graphics.beginFill(_fc, 1);	foo.graphics.drawCircle(0, 0, 14);	foo.graphics.drawCircle(-18, -18, 8);	foo.graphics.drawCircle(0, -26, 8);	foo.graphics.drawCircle(18, -18, 8);	addChild(foo);	return foo;}for(var i:int = 0; i < 4; i++){	shadowArr.push(creatFoot(0));	footArr.push(creatFoot(0xFFFFFF));	xArr.push(140 + 120 * (i % 2));	yArr.push(100 + 200 * int(i / 2));	footArr[i].x = xArr[i];	footArr[i].y = yArr[i];}run()function run(){	for(var i:int = 0; i < 4; i++)	{		TweenMax.to(footArr[order[i]], 0.4, {scaleX:1.4, scaleY:1.4, delay:0.5 * i, ease:Quad.easeIn, overwrite:false});		TweenMax.to(footArr[order[i]], 0.2, {scaleX:1, scaleY:1, delay:0.4 + 0.5 * i, ease:Quad.easeOut, overwrite:false});				TweenMax.to(footArr[order[i]], 0.6, {y:yArr[order[i]] - 40, delay:0.5 * i, ease:Sine.easeInOut, overwrite:false});		TweenMax.to(footArr[order[i]], 1, {y:yArr[order[i]] + 60, delay:0.6 + 0.5 * i, ease:Sine.easeInOut, overwrite:false});	}		TweenMax.delayedCall(2, run);}addEventListener(Event.ENTER_FRAME, follow);function follow(e:Event = null){	for(var i:int = 0; i < 4; i++)	{		shadowArr[i].x = footArr[i].x + (footArr[i].scaleX - 1) * 30;		shadowArr[i].y = footArr[i].y + (footArr[i].scaleY - 1) * 50;		shadowArr[i].alpha = 1 - (footArr[i].scaleX - 1) * 1.5;		shadowArr[i].scaleX = 1 + (footArr[i].scaleX - 1) * 1.5;		shadowArr[i].scaleY = 1 + (footArr[i].scaleY - 1) * 1.5;			}}