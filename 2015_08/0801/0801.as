import com.greensock.TweenMax;import com.greensock.easing.*;import com.greensock.plugins.*; TweenPlugin.activate([MotionBlurPlugin]); var mb = {strength:0.5, fastMode:false, padding:15};var ball = new Shape();var high:Number = 200;ball.graphics.beginFill(0, 1);ball.graphics.drawCircle(0, 0, 10);addChild(ball);TweenMax.delayedCall(1, run);function run(){	high = 200 - Math.random() * 60;	TweenMax.fromTo(ball, 0.2, {x:-20, y:360}, {x:420, y:200, ease:Linear.easeNone, motionBlur:true});	TweenMax.delayedCall(0.195, bounce);}function bounce(){	trace(high);	TweenMax.to(ball, 1, {x:-20, ease:Quad.easeOut, motionBlur:mb, overwrite:false});	TweenMax.to(ball, 0.1, {y:400 - (400 - high) * 1.05, ease:Quad.easeOut, motionBlur:mb, overwrite:false});	TweenMax.to(ball, 0.9, {y:390, ease:Bounce.easeOut, delay:0.1, onComplete:run, overwrite:false});}