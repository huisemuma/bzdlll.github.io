import com.greensock.TweenMax;import com.greensock.easing.*;import com.greensock.plugins.*; TweenPlugin.activate([MotionBlurPlugin]); var lineC:uint = 0xAAAAAA;var W:Number = 152.5;var H:Number = 274;var r:Number = 3;var xRange:Number = 10;var yRange:Number = 12;var tRange:Number = 3;var blurStrength:Number = 0.3;var point = new PerspectiveProjection();point.projectionCenter = new Point(stage.stageWidth/2, stage.stageHeight/8);point.fieldOfView = 80;this.transform.perspectiveProjection = point;var table:Shape = new Shape();addChild(table);table.graphics.lineStyle(4, lineC, 1, false, LineScaleMode.NONE, CapsStyle.NONE);table.graphics.beginFill(0x555555, 1);table.graphics.drawRect((400 - W) / 2, (400 - H) / 2, W, H);table.graphics.endFill();table.graphics.moveTo(200, (400 - H) / 2);table.graphics.lineTo(200, 400 - (400 - H) / 2);table.graphics.lineStyle(8, 0, 0.6);table.graphics.moveTo(110, 204);table.graphics.lineTo(290, 204);table.graphics.lineStyle(2, lineC, 1);table.graphics.moveTo(110, 200);table.graphics.lineTo(290, 200);var ballShadow:Shape = new Shape();ballShadow.graphics.beginFill(0, 0.8);ballShadow.graphics.drawCircle(0, 0, r * 1.2);var ball:MovieClip = new MovieClip();var core:Shape = new Shape();core.graphics.beginFill(0xFFFFFF, 1);core.graphics.drawCircle(0, 0, r);addChild(ball);ball.addChild(ballShadow);ball.addChild(core);Start();function Start(){	TweenMax.to(ball, 0, {x:150, y:30});	TweenMax.fromTo(core, 0.5 * tRange, {scaleX:0, scaleY:0}, {scaleX:4, scaleY:4, ease:Quad.easeOut, overwrite:false});	TweenMax.to(core, 0.5 * tRange, {scaleX:2, scaleY:2, delay:0.5 * tRange, ease:Quad.easeIn, overwrite:false, onComplete:Round1});	TweenMax.fromTo(ballShadow, 0.5 * tRange, {x:0, y:0}, {alpha:0.3, x:4 * xRange, y:4 * yRange, ease:Quad.easeOut, overwrite:false});	TweenMax.to(ballShadow, 0.5 * tRange, {alpha:0.5, x:2 * xRange, y:2 * yRange, delay:0.5 * tRange, ease:Quad.easeIn, overwrite:false});	}function Round1(){	TweenMax.to(ball, 0.3 * tRange, {x:250, y:300, motionBlur:{strength:blurStrength}, ease:Linear.easeNone, onComplete:Round2});	Bounce3(0.3 * tRange, 2);}function Round2(){	TweenMax.to(ball, 0.2 * tRange, {x:260, y:100, motionBlur:{strength:blurStrength}, ease:Linear.easeNone, onComplete:Round3});	Bounce2(0.2 * tRange, 2);}function Round3(){	TweenMax.to(ball, 0.2 * tRange, {x:130, y:310, motionBlur:{strength:blurStrength}, ease:Linear.easeNone, onComplete:Round4});	Bounce2(0.2 * tRange, 2);}function Round4(){	TweenMax.to(ball, 0.2 * tRange, {x:180, y:55, motionBlur:{strength:blurStrength}, ease:Linear.easeNone, onComplete:Round5});	Bounce2(0.2 * tRange, 2);}function Round5(){	TweenMax.to(ball, 0.1 * tRange, {x:260, y:310, motionBlur:{strength:blurStrength}, ease:Linear.easeNone, onComplete:RoundOver});	Bounce2(0.1 * tRange, 2);}function RoundOver(){	TweenMax.to(ball, 0.12 * tRange, {x:460, y:360, ease:Linear.easeNone});	TweenMax.to(core, 0.15 * tRange,  {scaleX:10, scaleY:10, ease:Quad.easeOut, overwrite:false});	TweenMax.to(ballShadow, 0.15 * tRange, {x:10 * xRange, y:10 * yRange, ease:Quad.easeOut, overwrite:false});		TweenMax.delayedCall(0.4 * tRange, Start);}function Bounce3(_duration, _scale){	TweenMax.to(core, _duration/4, {scaleX:1, scaleY:1, ease:Linear.easeNone, overwrite:false});	TweenMax.to(core, _duration/4, {scaleX:_scale, scaleY:_scale, delay:_duration/4, ease:Quad.easeOut, overwrite:false});	TweenMax.to(core, _duration/4, {scaleX:1, scaleY:1, delay:_duration/2, ease:Quad.easeIn, overwrite:false});	TweenMax.to(core, _duration/4, {scaleX:_scale/3*2, scaleY:_scale/3*2, delay:_duration/4*3, ease:Quad.easeOut, overwrite:false});	TweenMax.to(ballShadow, _duration/4, {alpha:1, x:0, y:0, ease:Linear.easeNone, overwrite:false});	TweenMax.to(ballShadow, _duration/4, {alpha:0.5, x:_scale * xRange, y:_scale * yRange, delay:_duration/4, ease:Quad.easeOut, overwrite:false});	TweenMax.to(ballShadow, _duration/4, {alpha:1, x:0, y:0, delay:_duration/2, ease:Quad.easeIn, overwrite:false});	TweenMax.to(ballShadow, _duration/4, {alpha:0.5, x:_scale/3*2 * xRange, y:_scale/3*2 * yRange, delay:_duration/4*3, ease:Quad.easeOut, overwrite:false});}function Bounce2(_duration, _scale){	TweenMax.to(core, _duration/3, {scaleX:_scale, scaleY:_scale, ease:Sine.easeOut, overwrite:false});	TweenMax.to(core, _duration/3, {scaleX:1, scaleY:1, delay:_duration/3*2, ease:Sine.easeIn, overwrite:false});	TweenMax.to(core, _duration/3, {scaleX:_scale/3*2, scaleY:_scale/3*2, delay:_duration/3*2, ease:Quad.easeOut, overwrite:false});		TweenMax.to(ballShadow, _duration/3, {alpha:0.5, x:_scale * xRange, y:_scale * yRange, ease:Quad.easeOut, overwrite:false});	TweenMax.to(ballShadow, _duration/3, {alpha:1, x:0, y:0, delay:_duration/3, ease:Quad.easeIn, overwrite:false});	TweenMax.to(ballShadow, _duration/3, {alpha:0.5, x:_scale/3*2 * xRange, y:_scale/3*2 * yRange, delay:_duration/3*2, ease:Quad.easeOut, overwrite:false});	}