import com.greensock.TweenMax;import com.greensock.easing.*;import com.greensock.plugins.*;TweenPlugin.activate([MotionBlurPlugin]);/*import com.greensock.plugins.*; TweenPlugin.activate([MotionBlurPlugin]); */stage.color = 0;this.graphics.lineStyle(10, 0xFFFFFF, 1, false, LineScaleMode.NONE, CapsStyle.NONE);this.graphics.moveTo(0, 250);this.graphics.lineTo(400, 250);this.graphics.lineStyle(4, 0xFFFFFF, 1, false, LineScaleMode.NONE, CapsStyle.NONE);this.graphics.moveTo(200, 250);this.graphics.lineTo(200, 220);var fallX:Number = 0;var ball:Shape = new Shape();ball.graphics.beginFill(0xFFFFFF, 1);ball.graphics.drawCircle(0, 0, 5);addChild(ball);f1();function f1(){	TweenMax.to(ball, 0.2, {x:80, y:242, motionBlur:{strength:0.2, fastMode:true}, onComplete:f2});}function f2()//左触案弹起{	fallX = 300 + Math.random() * 60;		TweenMax.to(ball, 0.06, {x:20, y:200, motionBlur:{strength:0.2, fastMode:true}});		TweenMax.delayedCall(0.05, f3);}function f3(){	TweenMax.to(ball, 1, {x:fallX, ease:Linear.easeNone, motionBlur:{strength:0.3, fastMode:true}, overwrite:false});	TweenMax.to(ball, 0.5, {y:50, ease:Quad.easeOut, motionBlur:{strength:0.2, fastMode:true}, overwrite:false});	TweenMax.to(ball, 0.5, {y:242, ease:Quad.easeIn, delay:0.5, motionBlur:{strength:0.2, fastMode:true}, overwrite:false});		TweenMax.delayedCall(0.99, f4);}function f4()//右触案弹起{	TweenMax.to(ball, 0.1, {x:fallX + 30, y:180, ease:Linear.easeNone, motionBlur:{strength:0.2, fastMode:true}});	TweenMax.delayedCall(0.09, f5);}function f5(){	TweenMax.to(ball, 0.16, {x:80, ease:Linear.easeNone, motionBlur:{strength:0.2, fastMode:true}, overwrite:false});	TweenMax.to(ball, 0.16, {y:242, ease:Sine.easeIn, motionBlur:{strength:0.2, fastMode:true}, overwrite:false});	TweenMax.delayedCall(0.15, f2);}